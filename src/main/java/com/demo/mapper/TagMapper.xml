<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.dao.TagMapper">

    <select id="selectAllTags" resultType="com.demo.entity.Tag">
        select * from tag where account=#{account};
    </select>

    <insert id="insertTag" parameterType="com.demo.entity.Tag">
        insert into tag values(#{account}, #{name});
    </insert>

</mapper>