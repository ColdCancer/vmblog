<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.dao.ArticleMapper">

    <select id="selectByAccount" resultType="com.demo.pojo.Article">
        select * from article where account=#{account};
    </select>

    <select id="selectFirstPostArticle" resultType="com.demo.pojo.Article">
        select * from article order by post_time desc limit 1;
    </select>

</mapper>